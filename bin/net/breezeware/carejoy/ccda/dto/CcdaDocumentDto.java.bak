package net.breezeware.carejoy.ccda.dto;

import java.io.Serializable;
import java.util.List;

import com.google.gson.Gson;

import net.breezeware.carejoy.domain.graph.entity.Patient;
import net.breezeware.carejoy.domain.graph.entity.Practitioner;
import net.breezeware.carejoy.domain.graph.entity.clinical.Allergy;
import net.breezeware.carejoy.domain.graph.entity.clinical.Condition;
import net.breezeware.carejoy.domain.graph.entity.clinical.Consent;
import net.breezeware.carejoy.domain.graph.entity.clinical.DiagnosticReport;
import net.breezeware.carejoy.domain.graph.entity.clinical.Encounter;
import net.breezeware.carejoy.domain.graph.entity.clinical.FamilyMemberHistory;
import net.breezeware.carejoy.domain.graph.entity.clinical.Immunization;
import net.breezeware.carejoy.domain.graph.entity.clinical.MedicationAdministration;
import net.breezeware.carejoy.domain.graph.entity.clinical.Procedure;
import net.breezeware.carejoy.domain.graph.entity.clinical.VitalSign;

/**
 * DTO to represent the components of a CCDA document. Currently 10 sections
 * that are part of the CCD template are included.
 * 
 * @author karthik
 *
 */
public class CcdaDocumentDto implements Serializable {

    /**
     * 
     */
    private static final long serialVersionUID = 1L;

    private Patient patient;

    private Practitioner practitioner;

    private List<Allergy> allergyList;

    private List<Condition> conditionList;

    private List<Procedure> procedureList;

    private List<VitalSign> vitalsList;

    private List<MedicationAdministration> medicationAdministrationList;

    private List<FamilyMemberHistory> familyMemberHistoryList;

    private List<Immunization> immunizationList;

    private List<DiagnosticReport> diagnosticReportList;

    private List<Consent> advanceDirectiveList;

    private List<Encounter> encounterList;

    public Patient getPatient() {
        return patient;
    }

    public void setPatient(Patient patient) {
        this.patient = patient;
    }

    public Practitioner getPractitioner() {
        return practitioner;
    }

    public void setPractitioner(Practitioner practitioner) {
        this.practitioner = practitioner;
    }

    public List<Allergy> getAllergyList() {
        return allergyList;
    }

    public void setAllergyList(List<Allergy> allergyList) {
        this.allergyList = allergyList;
    }

    public List<Condition> getConditionList() {
        return conditionList;
    }

    public void setConditionList(List<Condition> conditionList) {
        this.conditionList = conditionList;
    }

    public List<Procedure> getProcedureList() {
        return procedureList;
    }

    public void setProcedureList(List<Procedure> procedureList) {
        this.procedureList = procedureList;
    }

    public List<VitalSign> getVitalsList() {
        return vitalsList;
    }

    public void setVitalsList(List<VitalSign> vitalsList) {
        this.vitalsList = vitalsList;
    }

    public List<MedicationAdministration> getMedicationAdministrationList() {
        return medicationAdministrationList;
    }

    public void setMedicationAdministrationList(List<MedicationAdministration> medicationAdministrationList) {
        this.medicationAdministrationList = medicationAdministrationList;
    }

    public List<FamilyMemberHistory> getFamilyMemberHistoryList() {
        return familyMemberHistoryList;
    }

    public void setFamilyMemberHistoryList(List<FamilyMemberHistory> familyMemberHistoryList) {
        this.familyMemberHistoryList = familyMemberHistoryList;
    }

    public List<Immunization> getImmunizationList() {
        return immunizationList;
    }

    public void setImmunizationList(List<Immunization> immunizationList) {
        this.immunizationList = immunizationList;
    }

    public List<DiagnosticReport> getDiagnosticReportList() {
        return diagnosticReportList;
    }

    public void setDiagnosticReportList(List<DiagnosticReport> diagnosticReportList) {
        this.diagnosticReportList = diagnosticReportList;
    }

    public List<Consent> getAdvanceDirectiveList() {
        return advanceDirectiveList;
    }

    public void setAdvanceDirectiveList(List<Consent> advanceDirectiveList) {
        this.advanceDirectiveList = advanceDirectiveList;
    }

    public List<Encounter> getEncounterList() {
        return encounterList;
    }

    public void setEncounterList(List<Encounter> encounterList) {
        this.encounterList = encounterList;
    }

    @Override
    public String toString() {
        return new Gson().toJson(this);
    }
}